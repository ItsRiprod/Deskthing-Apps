{
  "name": "spotify",
  "private": true,
  "version": "0.11.2",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:wrapper\"",
    "build": "npx @deskthing/cli package",
    "build-server": "esbuild server/index.ts --bundle --platform=node --outfile=dist/index.js",
    "setup": "node setup.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:vite": "vite",
    "dev:wrapper": "npx @deskthing/cli dev",
    "build-workers": "esbuild server/workers/*.ts --bundle --platform=node --outdir=dist/workers --format=cjs"
  },
  "dependencies": {
    "@deskthing/client": "^0.11.0",
    "@deskthing/server": "^0.11.1",
    "dotenv": "^16.4.7",
    "lucide-react": "^0.482.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@deskthing/cli": "^0.11.4",
    "@deskthing/types": "^0.11.8",
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.6.1",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^4.3.1",
    "archiver": "^7.0.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.10",
    "tsm": "^2.3.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^6.3.5"
  }
}
